<Window x:Class="SortedListA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListA"
        mc:Ignorable="d"
       Title="MainWindow" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Margin="38,56,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="181" Height="24"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Margin="38,98,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="181" Height="24"/>
        <Button x:Name="AddClick" Content="Add" HorizontalAlignment="Left" Margin="255,57,0,0" VerticalAlignment="Top" Height="66" Width="94" Click="AddClick_Click"/>
    </Grid>
</Window>
